<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:property="http://ploop.org/namespaces/property#"
	xmlns:component="java:com.guiseframework.component."
	xmlns:layout="java:com.guiseframework.component.layout."
	xmlns:model="java:com.guiseframework.model."
	xmlns:validator="java:com.guiseframework.validator."
	xmlns:demo="java:com.guiseframework.demo."
>
	<demo:TemperatureConversionPanel2 property:name="temperatureConversionPanel">
		<property:layout><layout:FlowLayout property:flow="line"/></property:layout>
		<property:label>Guise&#x2122; Demonstration: Temperature Conversion</property:label>
		<property:children rdf:parseType="Collection">
			<component:LayoutPanel property:name="inputPanel">
				<property:layout><layout:FlowLayout property:flow="page"/></property:layout>
				<property:children rdf:parseType="Collection">
					<component:TextControl property:name="temperatureInput" property:valueClass="java.lang.Float" property:label="Input Temperature">
						<property:validator><validator:ValueRequiredValidator/></property:validator>
					</component:TextControl>
					<component:TextControl property:name="temperatureOutput" property:valueClass="java.lang.Float" property:label="Output Temperature" property:editable="false"/>
				</property:children>
			</component:LayoutPanel>
			<component:LayoutPanel property:name="conversionPanel">
				<property:layout><layout:FlowLayout property:flow="page"/></property:layout>
				<property:children rdf:parseType="Collection">
					<component:GroupPanel property:name="scalePanel" property:label="Input Scale">
						<property:layout><layout:FlowLayout property:flow="page"/></property:layout>
						<property:children rdf:parseType="Collection">
							<component:CheckControl property:name="celsiusCheckControl" property:checkType="ellipse" property:label="Calsius"/>
							<component:CheckControl property:name="fahrenheitCheckControl" property:checkType="ellipse" property:label="Fahrenheit"/>
						</property:children>
					</component:GroupPanel>
					<component:Button property:name="conversionButton" property:label="Convert"/>
				</property:children>
			</component:LayoutPanel>
		</property:children>
	</demo:TemperatureConversionPanel2>
</rdf:RDF>
