<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:property="http://ploop.org/namespaces/property#"
	xmlns:component="java:com.guiseframework.component."
	xmlns:layout="java:com.guiseframework.component.layout."
	xmlns:model="java:com.guiseframework.model."
	xmlns:validator="java:com.guiseframework.validator."
	xmlns:demo="java:com.guiseframework.demo."
>
	<demo:TemperatureConversionPanel2 rdf:ID="temperatureConversionPanel">
		<property:layout><layout:FlowLayout><property:flow>LINE</property:flow></layout:FlowLayout></property:layout>
		<property:label>Guise&#x2122; Demonstration: Temperature Conversion</property:label>
		<property:children rdf:parseType="Collection">
			<component:LayoutPanel rdf:ID="inputPanel">
				<property:layout><layout:FlowLayout><property:flow>PAGE</property:flow></layout:FlowLayout></property:layout>
				<property:children rdf:parseType="Collection">
					<component:TextControl rdf:ID="temperatureInput" property:valueClass="java.lang.Float" property:label="Input Temperature">
						<property:validator><validator:ValueRequiredValidator/></property:validator>
					</component:TextControl>
					<component:TextControl rdf:ID="temperatureOutput" property:valueClass="java.lang.Float" property:label="Output Temperature" property:editable="false"/>
				</property:children>
			</component:LayoutPanel>
			<component:LayoutPanel rdf:ID="conversionPanel">
				<property:layout><layout:FlowLayout><property:flow>PAGE</property:flow></layout:FlowLayout></property:layout>
				<property:children rdf:parseType="Collection">
					<component:GroupPanel rdf:ID="scalePanel" property:label="Input Scale">
						<property:layout><layout:FlowLayout><property:flow>PAGE</property:flow></layout:FlowLayout></property:layout>
						<property:children rdf:parseType="Collection">
							<component:CheckControl rdf:ID="celsiusCheckControl" property:checkType="ELLIPSE" property:label="Calsius"/>
							<component:CheckControl rdf:ID="fahrenheitCheckControl" property:checkType="ELLIPSE" property:label="Fahrenheit"/>
						</property:children>
					</component:GroupPanel>
					<component:Button rdf:ID="conversionButton" property:label="Convert"/>
				</property:children>
			</component:LayoutPanel>
		</property:children>
	</demo:TemperatureConversionPanel2>
</rdf:RDF>
